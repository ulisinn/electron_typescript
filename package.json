{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "ENV=development electron .",
    "server": "babel-node server.js",
    "dev": "concurrently -k 'babel-node server.js' 'npm start'",
    "build": "webpack --config webpack.config.production.js && electron-packager . ReactTypescript --platform=darwin --arch=all --prune --overwrite "

  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/electron/electron-quick-start.git"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/electron/electron-quick-start/issues"
  },
  "homepage": "https://github.com/electron/electron-quick-start#readme",
  "dependencies": {
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "typescript": "^1.8.10",
    "typings": "^1.0.4"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "biguint-format": "^1.0.0",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.9",
    "concurrently": "^2.1.0",
    "css-loader": "^0.23.1",
    "electron-builder": "^3.25.0",
    "electron-prebuilt": "^1.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "flake-idgen": "^1.0.0",
    "html-webpack-plugin": "^2.17.0",
    "html-webpack-template": "^5.0.0",
    "moment": "^2.13.0",
    "node-sass": "^3.7.0",
    "postcss-loader": "^0.9.1",
    "radio": "^0.2.0",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.2.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "tslint": "^3.10.2",
    "ts-promise": "^0.3.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.13.0"
  }
}
