{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "sourceMap": true,
    "jsx": "react",
    "noImplicitAny": true
  },
  "filesGlob": [
    "src/**/*.tsx",
    "src/**/*.ts",
    "typings/**/*d.ts"
  ],
  "files": [
    "./typings/index.d.ts",
    "./app/app.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
